@{
    ViewData["Title"] = "Projects";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <style>
        .nav_projects{
            font-size: small;
            font-weight: 500;
            color:#999999;
        }
        .nav_projects:hover{
            color:#505050;
            border-bottom: solid;
            border-bottom-color: blue;
            border-bottom-width:2px ;
        }
        .nav_projects_active{
            color:#505050;
            border-bottom: solid;
            border-bottom-color: blue;
            border-bottom-width:2px ;
        }
    </style>
</head>

<section class="content">
    <div class="container-fluid">
        <h5><b>Projects</b></h5><br />
        <div class="row">
            <div class="col-sm-3">
                <div class="input-group mb-3">
                    <div class="input-group-prepend">
                        <span class="input-group-text"><i class="fa fa-folder" style="color:#17a2b8"></i></span>
                    </div>
                    <select class="form-control">
                        <option selected>All projects</option>
                        <option>Your projects</option>
                        <option>Shared with you</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-1">
                <span class="nav_projects nav_projects_active">All</span>
            </div>
            <div class="col-sm-1">
                <span class="nav_projects">Designs</span>
            </div>
            <div class="col-sm-1">
                <span class="nav_projects">Images</span>
            </div>
            <div class="col-sm-1">
                <span class="nav_projects">Videos</span>
            </div>

        </div><br />
        <h5><b>Recent</b></h5>
        <div class="row">
            <div class="col-sm-2" style="height:150px; padding:5px;">
                <div style="background-color:rgb(235,235,235);padding:20px; height:100%;width:100%">
                    <img src="~/dist/img/tom-morbey-H7mzAJ8jTMg-unsplash.jpg" class="template-img" style="border-radius:6px;width:100%;height:100%" alt="Image">
                </div>
                <center style="font-size:12px;font-weight:600;">Title</center>
            </div>
            <div class="col-sm-2" style="height:150px; padding:5px;">
                <div style="background-color:rgb(235,235,235);padding:20px; height:100%;width:100%">
                    <video class="template-img" loop autoplay muted style="border-radius:6px;width:100%;height:100%">
                        <source src="~/dist/Videos/cat_walks_toward_camera_512kb.mp4"
                                type="video/mp4">
                        <source src="~/dist/Videos/cat_walks_toward_camera_512kb.ogg"
                                type="video/ogg">
                    </video>
                </div>
                <center style="font-size:12px;font-weight:600;">Title</center>
            </div>
            <div class="col-sm-2" style="height:150px; padding:5px;">
                <div style="background-color:rgb(235,235,235);padding:20px; height:100%;width:100%">
                    <video class="template-img" loop autoplay muted style="border-radius:6px;width:100%;height:100%">
                        <source src="~/dist/Videos/Canvas.move_.mp4"
                                type="video/mp4">
                        <source src="~/dist/Videos/Canvas.move_.ogg"
                                type="video/ogg">
                    </video>
                </div>
                <center style="font-size:12px;font-weight:600;">Title</center>
            </div>
            <div class="col-sm-2" style="height:150px; padding:5px;">
                <div style="background-color:rgb(235,235,235);padding:20px; height:100%;width:100%">
                    <img src="~/dist/img/anita-austvika-chRIgTHFBy0-unsplash.jpg" class="template-img" style="border-radius:6px;width:100%;height:100%" alt="Image">
                </div>
                <center style="font-size:12px;font-weight:600;">Title</center>
            </div>
            <div class="col-sm-2" style="height:150px; padding:5px;">
                <div style="background-color:rgb(235,235,235);padding:20px; height:100%;width:100%">
                    <video class="template-img" loop autoplay muted style="border-radius:6px; width:100%;height:100%">
                        <source src="~/dist/Videos/recording (1).webm"
                                type="video/mp4">
                    </video>
                </div><center style="font-size:12px;font-weight:600;">Title</center>
            </div>
            <div class="col-sm-2" style="height:150px; padding:5px;">
                <div style="background-color:rgb(235,235,235);padding:20px; height:100%;width:100%">
                    <img src="~/dist/img/mitchell-orr-2G4_KMJ4b9s-unsplash.jpg" class="template-img" style="border-radius:6px; width:100%;height:100%" alt="Image">
                </div><center style="font-size:12px;font-weight:600;">Title</center>
            </div>
        </div>
        <br />
        <hr />
        <table class="table table-hover">
            <thead>
                <tr>
                    <th style="text-align:center">Name</th>
                    <th style="text-align:center">People</th>
                    <th style="text-align:center">Type</th>
                    <th style="text-align:center">Edited</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        <img src="~/dist/img/mitchell-orr-2G4_KMJ4b9s-unsplash.jpg" style="border-radius:6px;" width="60" height="60" alt="Image">
                        <span style="padding-top:30px;"><b>Untitled</b></span>
                    </td>
                    <td style="padding-top:30px;">John</td>
                    <td style="padding-top:30px;">Doe</td>
                    <td style="padding-top:30px;">john@example.com</td>
                </tr>
            </tbody>
        </table>
    </div>

</section>
<script>

    window.onload = function() {
        //document.getElementById("nav_gallery").classList.add("active");
    }
</script>